{
  "name": "react-init",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn && yarn build && cross-env NODE_ENV=production node dist/server.bundle.js",
    "dev": "cross-env NODE_ENV=development && concurrently \"webpack --watch --config config/isomorphic.dev.js\" \"node dist/server.bundle.js\"",
    "build": "cross-env NODE_ENV=production webpack --config config/isomorphic.prod.js",
    "test": "jest",
    "client:dev": "cross-env NODE_ENV=development webpack-dev-server --open --config config/client.dev.js",
    "client:prod": "cross-env NODE_ENV=production webpack --config config/client.prod.js",
    "client:test": "node config/client.test.js",
    "server:dev": "cross-env NODE_ENV=development concurrently \"webpack --config config/server.dev.js\" \"nodemon dist/server.bundle.js\"",
    "server:prod": "cross-env NODE_ENV=production webpack --config config/server.prod.js",
    "server:test": "node config/server.test.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "antd": "^3.13.6",
    "babel-plugin-import": "^1.11.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "d3": "^5.9.1",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "csv-loader": "^3.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "react-test-renderer": "^16.8.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "xml-loader": "^1.2.1"
  },
  "sideEffects": false
}
