    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "d3": "^5.9.2",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "react": "^16.8.4",
